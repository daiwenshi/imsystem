  </update>
  
  <select id="queryQuoteCustomer" resultMap="quoteCustomer">
  	SELECT c.*,
 	q.`cid` AS qcid,q.`column1` AS qcolumn1,q.`column2` AS qcolumn2,q.`column3` AS qcolumn3,q.`column4` AS qcolumn4,q.`column5` AS qcolumn5,
 	q.`id` AS qid,q.`price` AS qprice,q.`state` AS qstate,q.`svid` AS qsvid,q.`time` AS qtime,q.`uid` AS quid,q.`updatetime` AS qupdatetime
	 FROM customer c 
	LEFT JOIN (SELECT * FROM quotedprice WHERE state=0 AND svid=#{gid}) q 
	ON c.`id`=q.`cid` 
	<where>
		c.`state`=0
  		<if test="name!=null">	  	
  			AND c.`name` LIKE CONCAT('%',#{name},'%')
  		</if>
	</where>
  </select>
  
  <select id="queryQuoteGoods" resultMap="quoteGoods">
  	SELECT g.*,
 	q.`cid` AS qcid,q.`column1` AS qcolumn1,q.`column2` AS qcolumn2,q.`column3` AS qcolumn3,q.`column4` AS qcolumn4,q.`column5` AS qcolumn5,
 	q.`id` AS qid,q.`price` AS qprice,q.`state` AS qstate,q.`svid` AS qsvid,q.`time` AS qtime,q.`uid` AS quid,q.`updatetime` AS qupdatetime
	FROM goods gs 
	INNER JOIN goodsvalue g
	ON gs.id=g.gid
	LEFT JOIN (SELECT * FROM quotedprice WHERE state=0 AND cid=#{cid}) q
	ON g.`id`=q.`svid` 
	<where>
  		<if test="name!=null">	  	
  			AND gs.`name` LIKE CONCAT('%',#{name},'%')
  		</if>
	</where>
  </select>
  
