  <select id="queryQuoteByPage" resultMap="BaseResultMap">
  	SELECT q.* FROM quotedprice q,customer c,goodsvalue g ,goods gs 
  	<where>
  		q.state=0 AND q.`cid`=c.`id` AND q.`svid`=g.`id` AND g.`gid`=gs.`id` 
  		<if test="cname!=null">
  			AND c.`name` LIKE CONCAT('%',#{cname},'%')  
  		</if>
  		<if test="gname!=null">
  			AND gs.`name` LIKE CONCAT('%',#{gname},'%')  
  		</if>
  	</where>
  	
  </select>
  
  <select id="queryQuoteByCustomerId" resultMap="BaseResultMap">
  	SELECT q.* FROM quotedprice q,customer c,goodsvalue g ,goods gs 
  	
  	WHERE q.state=0 AND q.`cid`=c.`id` AND q.`svid`=g.`id` AND g.`gid`=gs.`id` 
  		  	
  	AND c.`id`=#{cid}
  </select>
  
  <select id="queryQuoteByGoodsValueId" resultMap="BaseResultMap">
