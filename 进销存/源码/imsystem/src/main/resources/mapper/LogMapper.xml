	<select id="queryRiZhi" resultMap="ResultMap">
		SELECT l.*,u.name AS uname
		FROM `log` AS l
		INNER JOIN `user` AS u
		ON l.userid=u.id
		ORDER BY l.time
		DESC
		LIMIT 7
	</select>



